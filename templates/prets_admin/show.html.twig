{% extends 'base.html.twig' %}

{% block title %}Prets{% endblock %}

{% block body %}
    <h1>Prets</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pret.id }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ pret.dateFin ? pret.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ pret.dateDebut ? pret.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateRendu</th>
                <td>{{ pret.dateRendu ? pret.dateRendu|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Extension</th>
                <td>{{ pret.extension ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prets_admin_index') }}">back to list</a>

    <a href="{{ path('app_prets_admin_edit', {'id': pret.id}) }}">edit</a>

    {{ include('prets_admin/_delete_form.html.twig') }}
{% endblock %}
